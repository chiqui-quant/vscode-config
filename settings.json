  // Vim Keybindings
  // normal mode and visual mode tab indent
  "vim.normalModeKeyBindings": [
    {
      "before": ["Tab"],
      "after": [">",">","<Esc>"]
    },
    {
      "before": ["<"],
      "after": ["<","<"]
    },
    {
      "before": ["s"],
      "after": ["d","d"]
    },
    {
      "before": ["c","w"],
      "after": ["c","i","w"]
    },
  ],
