{
  "python.defaultInterpreterPath": "C:\\Users\\Chiqui\\AppData\\Local\\Microsoft\\WindowsApps\\python3.9.exe",
  "jupyter.interactiveWindowMode": "perFile",
  "jupyter.askForKernelRestart": false,
  "notebook.output.textLineLimit": 200,
  "latex-workshop.view.pdf.viewer": "tab",
  "r.rpath.windows": "C:\\Program Files\\R\\R-4.1.2\\bin\\R.exe",
  "editor.minimap.enabled": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  // "editor.quickSuggestionsDelay": 1000,
  "latex-workshop.latex.pdfWatch.delay": 1,
  "latex-workshop.view.pdf.zoom": "page-fit",
  "explorer.autoReveal": false, // do not autofocus in the current file when using filexplorer
  
  // Hide scrollbar
  "editor.scrollbar.verticalScrollbarSize": 0,
  "workbench.activityBar.visible": false,
  "vscode-office.openOutline": false,
  "editor.cursorSurroundingLines": 5,
  "editor.cursorBlinking": "solid",
  "workbench.startupEditor": "none",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.selectionHighlight": false,
  "editor.occurrencesHighlight": false,
  "workbench.colorCustomizations": {
  "editorCursor.foreground": "#858585", // custom cursor color
  },
  "workbench.colorTheme": "One Dark Pro Flat",
  "debug.disassemblyView.showSourceCode": false,
  "terminal.integrated.commandsToSkipShell": [
      "language-julia.interrupt"
  ],
  "julia.symbolCacheDownload": true,
  "memo.backlinksPanel.collapseParentItems": true,
  "breadcrumbs.enabled": false,
  "window.zoomLevel": -1,
  "telemetry.telemetryLevel": "off",
  
  // Vim settings
  "vim.useSystemClipboard": true,
  "vim.foldfix": true,
  "editor.wordWrap": "on",
  "audioCues.lineHasFoldedArea": "off",
  "editor.foldingHighlight": false,
  // Vim Keybindings
  "vim.insertModeKeyBindings": [
  {
  "before": ["'"],
  "commands": [
  {
    "command": "editor.action.insertSnippet",
    "args": { "snippet": "'$1'"} // autoclose quotes (also working in markdown)
  }
  ],
  },  
  {
  "before": ["\""],
  "commands": [
  {
    "command": "editor.action.insertSnippet",
    "args": { "snippet": "\"$1\""} // autoclose quotes (also working in markdown)
  }
  ],
  },
  {
  "before": ["("],
  "commands": [
  {
    "command": "editor.action.insertSnippet",
    "args": { "snippet": "($1)"} // autoclose brackets
  }
  ],
  },  
  {
  "before": ["["],
  "commands": [
  {
    "command": "editor.action.insertSnippet",
    "args": { "snippet": "[$1]"} // autoclose brackets
  }
  ],
  },  
  {
  "before": ["{"],
  "commands": [
  {
    "command": "editor.action.insertSnippet",
    "args": { "snippet": "{$1}"} // autoclose brackets
  }
  ],
  },  
  {
  "before": ["<"],
  "commands": [
  {
    "command": "editor.action.insertSnippet",
    "args": { "snippet": "<$1>"} // autoclose tags
  }
  ],
  },  
  ],
  "julia.enableTelemetry": false,
  "julia.executablePath": "C:\\Users\\Chiqui\\AppData\\Local\\Programs\\Julia-1.7.3\\bin\\julia.exe",
}